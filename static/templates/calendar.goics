{{- /*gotype: github.com/SimonSchneider/chore-tracker/internal/core.ChoreListCalendarView*/ -}}
BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//chore-tracker//EN

{{ range .Chores }}
BEGIN:VEVENT
UID:{{ .ID }}@chore-tracker
DTSTAMP:{{ .LastCompletion.ToStdTime.Format "20060102" }}T000000Z
DTSTART;VALUE=DATE:{{ .NextCompletion.ToStdTime.Format "20060102" }}
DTEND;VALUE=DATE:{{ (.NextCompletion.Add 1).ToStdTime.Format "20060102" }}
SUMMARY:{{ .Name }}
END:VEVENT
{{ end }}

END:VCALENDAR